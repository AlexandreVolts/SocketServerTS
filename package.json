{
  "name": "SocketServerTS",
  "version": "1.0.0",
  "description": "An encapsulation of socket.io & TCPSocket to simplify real-time cross-plateform app development.",
  "repository": "https://github.com/AlexandreVolts/SocketServerTS.git",
  "scripts": {
    "deps:build": "yarn --cwd ../etwin-api-types run lib:build",
    "lib:build": "tsc -p src/lib/tsconfig.json",
    "lib:run": "yarn run lib:build && node ./build/lib/haxe/serializer.js"
  },
  "author": "Alexandre Cochet",
  "license": "GNU",
  "dependencies": {
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.8",
    "@types/socket.io": "^2.1.4"
  }
}
